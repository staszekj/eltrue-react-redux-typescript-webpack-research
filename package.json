{
  "name": "eltrue-react-redux-typescript-webpack-starter",
  "version": "0.0.1",
  "description": "Fork from piotrek.witek@gmail.com react-redux-typescript-webpack-starter",
  "author": "Stanislaw Jagiella <stanislaw.jagiella@gmail.com> (https://github.com/staszekj)",
  "homepage": "",
  "repository": "",
  "license": "MIT",
  "main": "dist/app.js",
  "scripts": {
    "clean": "rimraf dist",
    "build": "yarn run clean && webpack-dashboard -- webpack --config configs/webpack.config.js --env.build --env.sourceMap",
    "dev": "webpack-dev-server --config configs/webpack.config.js --open",
    "dev:dashboard": "webpack-dashboard -m -- webpack-dev-server --config configs/webpack.config.js --open",
    "lint": "tslint --project tsconfig.json",
    "tsc": "tsc -p . --noEmit",
    "tsc:watch": "tsc -p . --noEmit -w",
    "test": "jest --config jest.json",
    "test:watch": "jest --config jest.json --watch",
    "test:update": "jest --config jest.json -u",
    "reinstall": "rm -rf node_modules && yarn",
    "reinstall:win": "rd /s /q node_modules && yarn",
    "deploy": "echo 'deploy not set!'",
    "graph": "tsc && madge --image graph.svg out/index.js --warning"
  },
  "dependencies": {
    "ajv": "^6.4.0",
    "babel-polyfill": "^6.26.0",
    "material-ui": "^0.20.0",
    "react": "16.2.0",
    "react-dom": "^16.3.2",
    "react-redux": "^5.0.7",
    "react-router-dom": "4.2.2",
    "react-router-redux": "5.0.0-alpha.9",
    "redux": "3.7.2",
    "redux-observable": "0.17.0",
    "reselect": "3.0.1",
    "rxjs": "5.5.6",
    "tslib": "1.8.1",
    "typesafe-actions": "1.1.2",
    "utility-types": "1.0.0-rc.2"
  },
  "devDependencies": {
    "@types/enzyme": "3.1.6",
    "@types/jest": "22.0.1",
    "@types/material-ui": "^0.21.2",
    "@types/react": "^16.3.13",
    "@types/react-dom": "^16.0.5",
    "@types/react-redux": "^5.0.19",
    "@types/react-router-dom": "4.2.3",
    "@types/react-router-redux": "5.0.11",
    "@types/webpack": "3.8.2",
    "@types/webpack-dev-server": "2.9.2",
    "@types/webpack-env": "1.13.3",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-react-css-modules": "^3.4.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "css-loader": "^0.28.11",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "eslint": "^4.19.1",
    "eslint-plugin-react": "^7.7.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "0.14.3",
    "jest": "22.1.1",
    "node-sass": "^4.9.0",
    "rimraf": "2.6.2",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.21.0",
    "ts-jest": "22.0.1",
    "ts-loader": "^4.3.0",
    "tslint": "5.9.1",
    "tslint-react": "3.4.0",
    "typescript": "2.6.2",
    "webpack": "^4.8.1",
    "webpack-cli": "^2.1.3",
    "webpack-dashboard": "^0.4.0",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-dev-server": "^3.1.4"
  }
}
